{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [
        200
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "trailing-comma": false,
    "jsx-alignment": true,
        "jsx-equals-spacing": [true, "never"],
        "jsx-no-string-ref": true,
        "jsx-self-close": true
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        200
      ]
    }
  }
}